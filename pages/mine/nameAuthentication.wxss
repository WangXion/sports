/**feedBack.wxss**/
.container{
    background: #F7F4F8;
    box-sizing: border-box;
    height: 100vh;
}
.header{
    width: 100%;
    height: 414rpx;
}
.feedBack{
    margin: 0;
    padding: 0;
}
.feedBack .item{
    width: 100%;
    height: 101rpx;
    padding-left: 16rpx;
    box-sizing: border-box;
    background: #fff;
}
.feedBack .item:last-child{
    margin-bottom: 0;
}
.feedBack .content{
    height: 275rpx;
    align-items: flex-start;
    padding-top: 31rpx;
}
.feedBack .content textarea{
    padding-top: 5rpx;
    color: #333;
    opacity: 0.6;
}
.feedBack .item .label{
    font-size: 30rpx;
    color: #333;
    width: 145rpx;
}
.feedBack .item input{
    font-size: 30rpx;
    color: #333;
    opacity: 0.6;
}
.footer{
    width: 96%;
    height: 98rpx;
    box-sizing: border-box;
    margin: 0 auto;
    height: 98rpx;
    background: #FF791F;
    border-radius: 50rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 98rpx;
    margin-top: 65rpx;
}
.tip{
    padding: 0 16rpx;
    box-sizing: border-box;
    padding-top: 56rpx;
}
.tip .title{
    font-size: 30rpx;
    color: #000;
}
.tip .label{
    font-size: 24rpx;
    color: #333;
    opacity: 0.6;
}
.flexBetween{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.flexStart{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}