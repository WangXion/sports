/**index.wxss**/
.index {
  padding: 0 16rpx;
}
/* title、头像 */
.index-title {
  display: flex;
  justify-content: space-between;
  color:rgba(51,51,51,1);
  font-weight: 400;
  margin-bottom: 17rpx;
}
.index-title .left {
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.index-title .avatar {
  width: 56rpx;
  height: 56rpx;
  border-radius: 50%;
  background: #FFA546;
  margin-right: 20rpx;
}
.index-title .right {
  display: flex;
  align-items: center;
  font-size: 26rpx;
}
.index-title .right .icon {
  width: 25rpx;
  height: 32rpx;
  margin-left: 10rpx;
}
/* 轮播图 */
.index-banner {
  width: 720rpx;
  height: 269rpx;
  margin-bottom: 26rpx;
}
.bannerImageurl{
  border-radius: 10rpx;
}
/* 体质检测 入口 */
.index-icon {
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 26rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  margin-bottom: 45rpx;
}
.index-icon .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}
.index-icon .item .name {
  margin-top: 20rpx;
}
.index-icon .item .icon {
  width: 90rpx;
  height: 90rpx;
}
.index-icon .item .noOpen{
  width: 70rpx;
  height: 35rpx;
  background: url('https://cunjifen.oss-cn-beijing.aliyuncs.com/userImg/1597649456267.jpg') no-repeat;
  background-size: 100% 100%;
  position: absolute;
  right: -35rpx;
  top: -3rpx;
}
/* 体育头条 */
.index-ports {
  width: 677rpx;
  height: 80rpx;
  line-height: 80rpx;
  background:rgba(252,240,232,1);
  border-radius: 10rpx;
  margin-bottom: 45rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  font-weight: 400;
  color: rgba(51,51,51,1);
  padding: 0 24rpx;
}
.index-ports .swiper-box1 {
  width: 720rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.index-ports .icon {
  width: 53rpx;
  height: 53rpx;
  line-height: 80rpx;
  margin: 0 28rpx 0 0;
}
.index-ports .item {
  display: flex;
  justify-content: space-between;
}
.index-ports .title{
  width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  font-weight: 400;
}
.index-ports .right {
  color: #999999;
}
.index-venues {
  font-size: 32rpx;
  font-weight: bold;
  color:rgba(51,51,51,1);
  margin-bottom: 30rpx;
}
.index-venues .cont {
  margin-top: 30rpx;
  margin-left: 10rpx;
}
.index-venues .cont .item {
  /* box-shadow: 0rpx 0rpx 10rpx 0rpx rgba(0, 0, 0, 0.21); */
  /* border-radius: 10rpx; */
  height: 180rpx;
  box-sizing: border-box;
  /* padding-bottom: 20rpx; */
}
.index-venues .cont .item .icon{
  width: 270rpx;
  height: 180rpx;
  border-radius: 10rpx;
  margin-right: 30rpx;
}
.index-venues .cont .item .name {
  font-size: 32rpx;
  font-weight: 400;
  padding: 0 10rpx;
  margin-bottom: 10rpx;
  color:rgba(27,27,27,1);
}
.index-venues .cont .item .section {
  display: flex;
  margin-bottom: 10rpx;
  padding: 0 10rpx;
}
.index-venues .cont .item .section .tag {
  height: 36rpx;
  line-height: 36rpx;
  padding: 0rpx 12rpx;
  font-size: 24rpx;
  color: #fff;
  background: rgba(255,177,53,1);
  margin-right: 10rpx;
}
.index-venues .cont .item .section .tag:nth-of-type(2n) {
  color: #fff;
  background: rgba(255,64,82,1);
}
.index-venues .cont .item .detail {
  font-size: 24rpx;
  color:  #999;
  padding: 0 10rpx;
  margin-bottom: 15rpx;
}
.index-venues .cont .item .km {
  font-size: 24rpx;
  color: #999;
}
.index-venues .item .select {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 26rpx;
}
.index-venues .item .select .selectCont {
  display: flex;
  align-items: center;
}
.index-venues .item .select .selectCont .tag {
  line-height: 32rpx;
  margin-right: 10rpx;
  padding: 6rpx 11rpx;
  border-radius: 3rpx;
  color: #66b2ff;
  font-size: 24rpx;
  border: 1rpx solid #66b2ff;
}
.index-fitness {
  font-size: 30rpx;
  color:rgba(51,51,51,1);
}
.index-fitness .title {
  font-size: 32rpx;
  font-weight: bold;
  margin: 60rpx 0 32rpx 0;
}
.index-fitness .section {
  display: flex;
  white-space: nowrap;
}
.index-fitness .cont {
  margin-right: 30rpx;
  display: inline-block;
  width: 382rpx;
}
.index-fitness .cont .desc{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
}
.index-fitness .cont .icon {
  width: 382rpx;
  height: 241rpx;
  margin-bottom: 20rpx;
  display: inline-block;
  border-radius: 10rpx;
  overflow: hidden;
}
.index-fitness .cont .desc {
  font-size: 28rpx;
}
.index-information .title {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 60rpx;
}
.index-information .cont {
  display: flex;
  justify-content: space-between;
  font-size: 26rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #E8E5E9;
}
.border-none {
  border-bottom: 0rpx solid #E8E5E9 !important;
}
.index-information .cont .left {
  display: flex;
  flex-direction: column;
  color:rgba(51,51,51,1);
}
.index-information .cont .left .top {
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.index-information .cont .left .desc {
  display: flex;
  color:rgba(153,153,153,1);
  margin-top: 30rpx;
  font-size: 26rpx;
}
.index-information .cont .left .desc .record {
  margin-right: 20rpx;
  color: #FFA546;
}
.index-information .cont .right {
  min-width: 240rpx;
  height: 142rpx;
  border-radius: 6rpx;
  overflow: hidden;
}
image {
  width: 100%;
  height: 100%;
}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
  margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  border-radius: 50%;
  margin-left: 6rpx;
  margin-top: 50rpx;
  justify-content: space-between;
}
.swiper-box .wx-swiper-dot::before{
  content: '';
  width: 10rpx;
  display: inline-flex;
  height: 10rpx;
  border-radius: 50%;
  flex-grow: 1; 
  background: rgba(255,255,255,0.1);
  border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
  width: 40rpx !important;
  display: inline-flex;
  height: 10rpx;
  background:rgba(255,255,255,1);   
}
/* 设置背景遮罩层样式 */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-radius: 10rpx;
  z-index: 9999;
}
.mask .button {
  width: 350rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 50rpx;
  background: red;
  color: #fff;
  text-align: center;
  font-size: 26rpx;
}
.mask .icon {
  width: 400rpx;
  height: 400rpx;
  position: absolute;
  top: 90rpx;
  left: 170rpx;
}
/* 设置关闭按钮图片的位置 */
.mask .closeImg {
  position: absolute;
  top: 600rpx;
  left: 335rpx;
}
/* 设置关闭按钮宽高 */
.mask .closeImg_image {
  width: 50rpx;
  height: 50rpx;
}
.textOver{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.empty{
  width: 100vw;
  height: 20rpx;
  background: #F7F4F8;
  transform: translateX(-16rpx);
}