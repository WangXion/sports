/* pages/examinate/examinate.wxss */
.examinate {
  background: #F7F4F8;
  width: 100vw;
  height: 100%;
}
/* tab栏 */
.examinate .tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex: 2;
  font-size: 30rpx;
  height: 99rpx;
  background:rgba(255,255,255,1);
}
.examinate .tab .item {
  display: flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  color:rgba(102,102,102,1);
}
.examinate .tab .active {
  color: #FF791F;
}
.examinate .tab .item .name {
  margin: 0 0 18rpx 6rpx;
}
.examinate .tab .item .slider {
  width: 66rpx;
  height: 6rpx;
  background:rgba(255,121,31,1);
  border-radius: 10rpx;
}
/* 预约须知 */
.examinate .cont {
  padding: 40rpx 20rpx;
  margin-bottom: 190rpx;
}
.examinate .cont .title {
  font-size: 30rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  margin-bottom: 20rpx;
  text-align: center;
}
.examinate .cont .section {
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  line-height: 60rpx;
}
/* 底部按钮 */
.examinate .button {
  width: 100%;
  height: 190rpx;
  background: #fff;
  padding: 21rpx 0;
  position: fixed;
  bottom: 0;
  left: 0;
}
.examinate .button .row {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
  margin: 20rpx 0 30rpx 0;
}
.examinate .button .icon {
  width: 42rpx;
  height: 42rpx;
  margin: 0 8rpx 0rpx 23rpx;
}
.examinate .button .btn {
  width: 720rpx;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  color:rgba(255,255,255,1) !important;
  background-color: #F78D1B !important;
  font-size: 33rpx;
  font-weight: 400;
  margin-left: 15rpx;
  border-radius: 100rpx;
}
.examinate .button .text {
  color: #fff !important;
}