/**setting.wxss**/
.container{
    background: #F7F4F8;
    box-sizing: border-box;
    height: 100vh;
}
.setting{
    background: #fff;
    padding: 0 16rpx;
    box-sizing: border-box;
    padding-top: 20rpx;
}
.setting .item{
    width: 100%;
    height: 122rpx;
    padding-left: 20rpx;
    box-sizing: border-box;
    border-top: 1px solid #F0F0F0;
}
.setting  .item:first-child{
    border-top: none;
}
.setting .item .label{
    font-size: 30rpx;
    color: #333;
}
.setting .item .val{
    font-size: 30rpx;
    color: #333;
    opacity: 0.6;
    padding-right: 14rpx;
}
.setting .item .arrow{
    width: 14rpx;
    height: 25rpx;
    margin-right: 24rpx;
}
.flexBetween{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.flexStart{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}