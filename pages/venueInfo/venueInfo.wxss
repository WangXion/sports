/* pages/venueInfo/venueInfo.wxss */
.venueInfo {

}
.venueInfo .title {
  display: flex;
  align-items: center;
  padding-left: 33rpx;
  font-size: 38rpx;
  font-weight: 400;
  color: #666;
  margin-bottom: 50rpx;
}
.venueInfo .title .icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
/* 内容区域 */
.venueInfo .cont {
  padding: 0 85rpx;
  
}
.venueInfo .config {
  flex-wrap: wrap;
}
.venueInfo .cont .start {
  justify-content: flex-start;
}
.venueInfo .config .item {
  font-size: 28rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
  text-align: center;
  width: 25%;
  margin-bottom: 50rpx;
}
.venueInfo .config .item .icon{
  width: 46rpx;
  height: 54rpx;
}
.mr0 {
  margin-right: 0 !important;
}
.venueInfo .cont .item .icon {
  width: 46rpx;
  height: 54rpx;
}
/* 车位情况 */
.venueInfo .car {
  margin-bottom: 50rpx;
}
.venueInfo .car .top{
  display: flex;
  justify-content: flex-start;
  font-size: 26rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  margin-bottom: 38rpx;
  margin-top: 66rpx;
}
.venueInfo .car .top .icon {
  width: 29rpx;
  height: 29rpx;
  margin-right: 15rpx;
}
.venueInfo .car .section {
  display: flex;
  justify-content: flex-start;
  font-size: 26rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
}
.venueInfo .car .section .left {
  width: 50%;
  display: flex;
  flex-direction: column;
}
.venueInfo .car .section .left .price {
  font-size: 38rpx;
  color:rgba(245,145,74,1);
  margin: 12rpx 0;
}
