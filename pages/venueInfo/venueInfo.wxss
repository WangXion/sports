/* pages/venueInfo/venueInfo.wxss */
.venueInfo {

}
.venueInfo .title {
  display: flex;
  align-items: center;
  padding-left: 33rpx;
  font-size: 36rpx;
  font-weight: 400;
  color:rgba(0,0,0,1);
  margin-bottom: 50rpx;
}
.venueInfo .title .icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
/* 内容区域 */
.venueInfo .cont {
  padding: 0 85rpx;
  
}
.venueInfo .cont .row {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.venueInfo .cont .start {
  justify-content: flex-start;
}
.venueInfo .cont .item {
  font-size: 26rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
  margin-bottom: 50rpx;
  margin-right: 115rpx;
  text-align: center;
}
.mr0 {
  margin-right: 0 !important;
}
.venueInfo .cont .item .icon {
  width: 46rpx;
  height: 54rpx;
}
/* 车位情况 */
.venueInfo .car {
  margin-bottom: 50rpx;
}
.venueInfo .car .top{
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  margin-bottom: 38rpx;
}
.venueInfo .car .top .icon {
  width: 29rpx;
  height: 29rpx;
  margin-right: 15rpx;
}
.venueInfo .car .section {
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
}
.venueInfo .car .section .left {
  width: 50%;
  display: flex;
  flex-direction: column;
}
.venueInfo .car .section .left .price {
  font-size: 36rpx;
  color:rgba(245,145,74,1);
  margin: 12rpx 0;
}
/* 其他信息 */
.venueInfo .other {
  margin-bottom: 50rpx;
}
.venueInfo .other .top{
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(51,51,51,1);
  margin-bottom: 38rpx;
}
.venueInfo .other .top .icon {
  width: 29rpx;
  height: 29rpx;
  margin-right: 15rpx;
}
.venueInfo .other .section {
  display: flex;
  justify-content: flex-start;
  margin-bottom: 20rpx;
}
.venueInfo .other .section .left {
  width: 50%;
  display: flex;
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(153,153,153,1);
}
.venueInfo .other .section .value {
  display: flex;
  font-size: 24rpx;
  font-weight: 400;
  color:#333;
}