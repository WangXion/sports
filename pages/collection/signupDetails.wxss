/**signupDetails.wxss**/
.container{
    background: #F7F4F8;
    box-sizing: border-box;
    height: 100vh;
}
.title{
    font-size: 30rpx;
    color: #000;
    height: 90rpx;
    line-height: 90rpx;
    padding-left: 15rpx;
}
.details{
    background: #fff;
    padding: 0 16rpx;
    box-sizing: border-box;
}
.details .item{
    width: 100%;
    height: 102rpx;
    padding-left: 20rpx;
    box-sizing: border-box;
    border-top: 1px solid #F0F0F0;
}
.details .remark{
    height: 250rpx;
    padding-top: 20rpx;
}
.details .remark .val{
    padding-top: 15rpx;
}
.details .item:first-child{
    border-top: none;
}
.details .item .label{
    font-size: 24rpx;
    color: #999;
}
.details .item .val{
    font-size: 26rpx;
    color: #666;
    padding-right: 14rpx;
}
.footer{
    width: 100%;
    position: fixed;
    bottom: 37rpx;
    box-sizing: border-box;
    z-index: 1;
}
.footer .btn{
    width: 96%;
    height: 98rpx;
    box-sizing: border-box;
    background: #F78D1B;
    border-radius: 50rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    line-height: 98rpx;
    margin: 0 auto;
    margin-bottom: 25rpx;
}
.flexBetween{
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.flexStart{
    display: flex;
    justify-content: flex-start;
    align-items: center;
}