/* pages/scientificFitness/scientificFitness.wxss */
.keep {
  margin-top: 15rpx;
  padding: 0 15rpx;
}
.keep .tab {
  display: flex;
  align-items: center;
  font-weight: 400;
  color:rgba(102,102,102,1);
}
.keep .tab .item{
  /* margin-right: 38rpx; */
  width: 99rpx;
  font-size: 30rpx;
}
.keep .tab .active {
  font-size: 36rpx;
  color: #000;
}
.keep .search {
  display: flex;
  width: 139rpx;
  height: 57rpx;
  line-height: 57rpx;
  background: rgba(237,237,237,1);
  font-size: 24rpx;
  font-weight: 400;
  color:rgba(102,102,102,1);
  border-radius: 40rpx;
}
.keep .search .icon {
  width: 30rpx;
  height: 30rpx;
  margin: 16rpx 19rpx 0 19rpx;
}
/* banner */
.keep .banner {
  width: 720rpx;
  height: 279rpx;
  margin: 40rpx 0 7rpx 0;
}
/* 列表 */
.keep .list {
  display: flex;
  justify-content: space-between;
  padding:  38rpx 0 25rpx 0;
  border-bottom: 2rpx solid #e5e5e5;
  padding-top: 35rpx;
}
.keep .list .left {
  font-size: 26rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.keep .list .left .bottom{
  margin-top: 30rpx;
  color:rgba(153,153,153,1);
}
.keep .list .left  .ml20 {
  margin-left: 20rpx;
}
.keep .list .right {
  min-width: 240rpx;
  height: 142rpx;
}