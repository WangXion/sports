/* pages/result/result.wxss */
.order {
    background: #F6F6F6;
    height: 100vh;
}
.order .picture{
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    background: #fff;
    padding-bottom: 70rpx;
}
.order .picture .img{
    width: 111rpx;
    height: 111rpx;
    padding-top: 62rpx;
}
.order .picture .success{
    color:#3CC56D;
    padding-top: 19rpx;
}
.order .picture .button{
    display: flex;
    margin-top: 70rpx;
    font-size: 30rpx;
    font-weight: 400;
}
.order .picture .button .return{
    width: 239rpx;
    height: 85rpx;
    line-height: 85rpx;
    background: rgba(215, 215, 215, 1);
    border-radius: 50rpx;
    color:rgba(102,102,102,1);
    text-align: center;
    margin-right: 55rpx;
}
.order .picture .button .see{
    width: 239rpx;
    height: 85rpx;
    line-height: 85rpx;
    background: rgba(255, 152, 31, 1);
    border-radius: 50rpx;
    color:#fff;
    text-align: center;
}