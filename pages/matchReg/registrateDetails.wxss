/* pages/registrateDetails/registrateDetails.wxss */
.registrateDetails {
  background: #f6f6f6;
  min-height: 100vh;
  padding-bottom: 40rpx;
}
.registrateDetails .detail .title {
  font-size: 30rpx;
  font-weight: 400;
  color:#1C1C1C;
  padding: 27rpx 0;
}

.registrateDetails .mainTitle{
  height: 76rpx;
  color: #000000;
  font-size: 30rpx;
  line-height: 76rpx;
  padding-left: 15rpx;
}
.registrateDetails .detail {
  font-size: 26rpx;
  color: #9A9A9A;
  box-sizing: border-box;
  padding: 0 16rpx;
  padding-bottom: 36rpx;
  background: #fff;
}
.registrateDetails .detail .desc {
  line-height: 50rpx;
}
/* 信息 */
.registrateDetails .section {
  background: #fff;
  padding: 0  22rpx 0 19rpx;
  /* margin-bottom: 100rpx; */
}
.registrateDetails .section .item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex: 2;
  font-size: 26rpx;
  font-weight: 400;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #e5e5e5;
}
.registrateDetails .section .item:last-child{
  border-bottom: none;
}
.registrateDetails .section .item .lable {
  color: #999;
}
.registrateDetails .section .item .value {
  color: #333;
  flex: 1;
  height: 100%;
  text-align: right;
}
.registrateDetails .section .item .input {
  line-height: 80rpx;
  text-align: right;
  height: 80rpx;
}
.registrateDetails .section .textarea {
  padding-top: 20rpx;
  color: #999;
  font-size: 24rpx;
}
.registrateDetails .section .textarea .lable {
  margin-bottom: 15rpx;
}
.registrateDetails .section .itemLeft{
  justify-content: flex-start;
  height: 280rpx;
  align-items: flex-start;
}
.registrateDetails .section .itemLeft .lableText{
  color: #999;
  margin-right: 20rpx;
}
.registrateDetails .section .itemLeft .cont {
  width: 205rpx;
  height: 156rpx;
  background:#EFEFEF;
  margin-top: 20rpx;
  /* flex: 1; */
}
.registrateDetails .section .itemLeft .uploadCont{
  flex-direction: column;
  color: #9A9A9A;
}
.registrateDetails .section .itemLeft .uploadCont image{
  width: 58rpx;
  height: 53rpx;
  margin-top: 20rpx;
}
.registrateDetails .fwText{
  background: #f6f6f6;
  margin-bottom: 20rpx;
  padding: 0 15rpx;
}
/* 我要报名 */
.registrateDetails .footer {
  width: 720rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #fff;
  background:rgba(247,141,27,1);
  text-align: center;
  margin-left: 16rpx;
  border-radius: 50rpx;
  font-size: 33rpx;
  /* position: fixed; */
  bottom: 30rpx;
}